<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.queomedia</groupId>
	<artifactId>queo-commons-exceptions</artifactId>
	<name>queo commons exceptions</name>
	<version>0.0.1-SNAPSHOT</version>
	<description>Exceptions used by queo commons and queo projects.</description>

	<issueManagement>
		<system>mantis</system>
		<url>http://mantistest.queo.local</url>
	</issueManagement>

	<scm>
		<connection>scm:svn:http://skylla/queo_commons/java/queo-commons-exceptions/trunk</connection>
		<developerConnection>scm:svn:http://skylla/queo_commons/java/queo-commons-exceptions/trunk</developerConnection>
		<url>http://skylla/queo_commons/java/queo-commons-exceptions/trunk</url>
	</scm>

	<developers>
		<developer>
			<organization>queo</organization>
			<name>Ralph Engelmann</name>
			<email>ralph.engelmann@queo-media.com</email>
		</developer>
	</developers>

	<distributionManagement>
		<snapshotRepository>
			<id>Snapshots</id>
			<name>Internal Snapshots</name>
			<url>http://localhost:8081/nexus/content/repositories/snapshots</url>
		</snapshotRepository>
	</distributionManagement>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

</project>